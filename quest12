numeros = list()
impares = 0
res = 's'
while res == 's':
    
    inteiro = int(input("Insira um número inteiro:"))
    numeros.append(inteiro)
    res = str(input('Quer continuar? [s/n] '))
    if inteiro % 2 != 0:
        impares = impares + 1

print("A quantidade de números impares é: {} ".format(impares))  








  









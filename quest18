valores = list()
prod = 1
cont = 0
res = 's'
while res == 's':
    cont = int(input('Digite um n√∫mero: '))
    valores.append(cont)
    res = str(input('Quer continuar? [s/n] '))
    
    prod = prod * cont
    cont = cont+1

print('O produto entre os valores informados foi {}'.format(prod))